script/source = "extends TextureRect


signal closed

var _shop: Shop

@onready var slot1: Button = $Slot1
@onready var slot2: Button = $Slot2
@onready var slot3: Button = $Slot3

@onready var slot1_price_type: TextureRect = $Slot1PriceType
@onready var slot2_price_type: TextureRect = $Slot2PriceType
@onready var slot3_price_type: TextureRect = $Slot3PriceType

@onready var slot1_price: Label = $Slot1Price
@onready var slot2_price: Label = $Slot2Price
@onready var slot3_price: Label = $Slot3Price


func _ready() -> void:
	set_process_input(false)


func set_player(player: Player) -> void:
	while true:
		_shop = await player.opening_shop
		_shop.force_close.connect(
			func():
				closed.emit()
		)
		slot1.icon = _shop.slot1_texture
		slot2.icon = _shop.slot2_texture
		slot3.icon = _shop.slot3_texture
		slot1_price_type.texture = price_type_to_texture(_shop.slot1_price_type)
		slot2_price_type.texture = price_type_to_texture(_shop.slot2_price_type)
		slot3_price_type.texture = price_type_to_texture(_shop.slot3_price_type)
		slot1_price.text = str(_shop.slot1_price)
		slot2_price.text = str(_shop.slot2_price)
		slot3_price.text = str(_shop.slot3_price)
		slot1.pressed.connect(select_slot1)
		slot2.pressed.connect(select_slot2)
		slot3.pressed.connect(select_slot3)
		show()
		set_process_input(true)
		await closed
		set_process_input(false)
		hide()
		slot1.pressed.disconnect(select_slot1)
		slot2.pressed.disconnect(select_slot2)
		slot3.pressed.disconnect(select_slot3)
		player.shop_closed()


func _input(event: InputEvent) -> void:
	if event.is_action_pressed(\"ui_cancel\"):
		closed.emit()


func select_slot1() -> void:
	_shop._on_texture_pressed(_shop.slot1_texture)
	slot1.icon = _shop.slot1_texture
	slot1_price_type.texture = price_type_to_texture(_shop.slot1_price_type)
	slot1_price.text = str(_shop.slot1_price)


func select_slot2() -> void:
	_shop._on_texture_pressed(_shop.slot2_texture)
	slot2.icon = _shop.slot2_texture
	slot2_price_type.texture = price_type_to_texture(_shop.slot2_price_type)
	slot2_price.text = str(_shop.slot2_price)


func select_slot3() -> void:
	_shop._on_texture_pressed(_shop.slot3_texture)
	slot3.icon = _shop.slot3_texture
	slot3_price_type.texture = price_type_to_texture(_shop.slot3_price_type)
	slot3_price.text = str(_shop.slot3_price)


func price_type_to_texture(type: Shop.PriceType) -> Texture2D:
	match type:
		Shop.PriceType.WIZARD_HATS:
			return preload(\"res://player/wizard_hat.png\")
	return preload(\"res://player/coin.png\")
"

[sub_resource type="GDScript" id="GDScript_pqjgk"]
script/source = "extends Node2D


@onready var idle: Sprite2D = $Node2D/Idle
@onready var delete: Sprite2D = $DeletePortal


func _input(event: InputEvent) -> void:
	if event is InputEventMouseMotion:
		position = get_global_mouse_position()

	elif event.is_action_pressed(\"attack\"):
		idle.hide()
	
	elif event.is_action_released(\"attack\"):
		idle.show()


func _process(_delta: float) -> void:
	position = get_global_mouse_position()
	
	if DeletablePortals.deletable_portals > 0:
		delete.show()
		idle.hide()
	else:
		delete.hide()
		idle.show()
"

[sub_resource type="GDScript" id="GDScript_7r6cs"]
script/source = "extends Node2D

var player: Player

@onready var idle: Sprite2D = $Idle


func _ready() -> void:
	set_process(false)


@warning_ignore(\"shadowed_variable\")
func set_player(player: Player) -> void:
	self.player = player
	set_process(true)
	


func _process(_delta: float) -> void:
	if player.poison_count == 0:
		idle.texture = preload(\"res://player/sword.png\")
	else:
		idle.texture = preload(\"res://player/poison-sword.png\")
	var player_viewport: SubViewport = player.get_viewport()
	var player_pos := player_viewport.get_camera_3d().unproject_position(player.global_position) / Vector2(player_viewport.size)
	rotation = (player_pos - get_parent().global_position / get_viewport_rect().size).angle()
"

